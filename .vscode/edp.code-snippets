{
	// Place your caderno-EDP workspace snippets here. Each snippet is defined under a snippet name and has a scope, prefix, body and 
	// description. Add comma separated ids of the languages where the snippet is applicable in the scope field. If scope 
	// is left empty or omitted, the snippet gets applied to all languages. The prefix is what is 
	// used to trigger the snippet and the body will be expanded and inserted. Possible variables are: 
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. 
	// Placeholders with the same ids are connected.
	// Example:
	// "Print to console": {
	// 	"scope": "javascript,typescript",
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	
	"Norm":{
		"scope": "latex",
		"prefix": "@norm",
		"body": ["\\|$1\\|_{$2}"],
		"description": "double vertical lines"
	},
	
	"Pairing":{
		"scope": "latex",
		"prefix": "@angle",
		"body": ["\\langle $1, $2 \\rangle"],
		"description": "two elements between angle brackets"
	},


	"Text em":{
		"scope": "latex",
		"prefix": "@t-em",
		"body": ["\\text{ em }"],
		"description": "em text"
	},
	
	
	
	
	"Partial U":{
		"scope": "latex",
		"prefix": "@pu",
		"body": ["\\partial U"],
		"description": "U border"
	},

	"Sobolev": {
		"scope": "latex",
		"prefix": "@Wsobolev",
		"body": [
			"W^{$1,$2}($3)"
		],
		"description": "Espaço de Sobolev"
	},

	"Sobolev loc": {
		"scope": "latex",
		"prefix": "@Wsoboloc",
		"body": [
			"W^{$1,$2}_{\\text{loc}}($3)"
		],
		"description": "Espaço de Sobolev loc"
	},


	"H⁻¹": {
		"scope": "latex",
		"prefix": "@h-1",
		"body": [
			"H^{-1}(U)"
		],
		"description": ""
	},

	"H¹_0": {
		"scope": "latex",
		"prefix": "@h10",
		"body": [
			"H^1_0(U)"
		],
		"description": ""
	}
